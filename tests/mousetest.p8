pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
	poke(0x5f2d, 1)
end

function _update()
	x = stat(32)
	y = stat(33)
	b = stat(34)
	w = stat(36)
end

function _draw()
	cls()
	print("x="..x)
	print("y="..y)
	print("b="..b)
	print("w="..w)
	spr(1, x-3, y-3)
	
end
